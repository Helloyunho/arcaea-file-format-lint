{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "https://raw.githubusercontent.com/yojohanshinwataikei/vscode-arcaea-file-format/master/json-schema/songlist.json",
	"title": "Arcaea songlist file",
	"description": "The file where Arcaea reads metadatas of songs",
	"type": "object",
	"properties": {
		"songs": {
			"description": "The list of metadata of songs",
			"type": "array",
			"items": {
				"$ref": "songdata.json"
			}
		}
	},
	"required": [
		"songs"
	]
}