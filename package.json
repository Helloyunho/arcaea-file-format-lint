{
	"name": "vscode-arcaea-file-format",
	"displayName": "Vscode Arcaea File Format Support",
	"description": "Util for reading and editing arcaea .aff files",
	"license": "MIT",
	"version": "0.1.1",
	"publisher": "yojohanshinwataikei",
	"repository": "github:yojohanshinwataikei/vscode-arcaea-file-format",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:arcaea-aff"
	],
	"main": "./client/out/extension",
	"contributes": {
		"languages": [
			{
				"id": "arcaea-aff",
				"aliases": [
					"Arcaea File Format",
					"arcaea-aff"
				],
				"extensions": [
					".aff"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "json",
				"filenames": [
					"songlist",
					"packlist",
					"unlocks"
				]
			}
		],
		"grammars": [
			{
				"language": "arcaea-aff",
				"scopeName": "source.arcaea-aff",
				"path": "./syntaxes/arcaea-aff.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "arcaea-aff",
				"path": "./snippets.json"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": "songlist",
				"url": "./json-scheme/songlist.json"
			},{
				"fileMatch": "songlist.json",
				"url": "./json-scheme/songlist.json"
			},
			{
				"fileMatch": "songdata.json",
				"url": "./json-scheme/songdata.json"
			},
			{
				"fileMatch": "packlist",
				"url": "./json-scheme/packlist.json"
			},
			{
				"fileMatch": "packlist.json",
				"url": "./json-scheme/packlist.json"
			},
			{
				"fileMatch": "packdata.json",
				"url": "./json-scheme/packdata.json"
			},
			{
				"fileMatch": "unlocks",
				"url": "./json-scheme/unlocks.json"
			},
			{
				"fileMatch": "unlocks.json",
				"url": "./json-scheme/unlocks.json"
			},
			{
				"fileMatch": "unlockdata.json",
				"url": "./json-scheme/unlockdata.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd .."
	},
	"devDependencies": {
		"@types/node": "^11.13.7",
		"typescript": "^3.4.5"
	}
}